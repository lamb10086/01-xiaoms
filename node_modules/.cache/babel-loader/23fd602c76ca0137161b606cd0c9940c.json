{"ast":null,"code":"import TodayKnowledge from \"@/views/xiaomengshu/TodayKnowledge.vue\";\nimport LittleTool from \"@/views/xiaomengshu/LittleTool.vue\";\nimport MemoCom from \"@/views/xiaomengshu/MemoCom.vue\";\nimport QuestionCom from \"@/views/xiaomengshu/QuestionCom.vue\";\nimport BottomColumn from \"@/views/xiaomengshu/BottomColumn.vue\";\nexport default {\n  data() {\n    return {\n      searchValue: \"\",\n      show: false,\n      column: []\n    };\n  },\n  computed: {\n    pet_type() {\n      return this.$store.getters.pet_type;\n    },\n    choiceAnimal() {\n      return this.$store.getters.choiceAnimal;\n    },\n    user() {\n      return this.$store.state.user;\n    }\n  },\n  methods: {\n    changed(item) {\n      this.show = false;\n      // close();\n      this.$store.state.pet_typeid = item - 1;\n    }\n  },\n  components: {\n    TodayKnowledge,\n    LittleTool,\n    MemoCom,\n    QuestionCom,\n    BottomColumn\n  }\n};","map":{"version":3,"mappings":"AAqEA,OAAOA,cAAa,MAAO,wCAAwC;AACnE,OAAOC,UAAS,MAAO,oCAAoC;AAC3D,OAAOC,OAAM,MAAO,iCAAiC;AACrD,OAAOC,WAAU,MAAO,qCAAqC;AAC7D,OAAOC,YAAW,MAAO,sCAAsC;AAC/D,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,IAAI,EAAE,KAAK;MACXC,MAAM,EAAE;IACV,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,QAAQ,GAAG;MACT,OAAO,IAAI,CAACC,MAAM,CAACC,OAAO,CAACF,QAAQ;IACrC,CAAC;IACDG,YAAY,GAAG;MACb,OAAO,IAAI,CAACF,MAAM,CAACC,OAAO,CAACC,YAAY;IACzC,CAAC;IACDC,IAAI,GAAG;MACL,OAAO,IAAI,CAACH,MAAM,CAACI,KAAK,CAACD,IAAI;IAC/B;EACF,CAAC;EACDE,OAAO,EAAE;IACPC,OAAO,CAACC,IAAI,EAAE;MACZ,IAAI,CAACX,IAAG,GAAI,KAAK;MACjB;MACA,IAAI,CAACI,MAAM,CAACI,KAAK,CAACI,UAAS,GAAID,IAAG,GAAI,CAAC;IACzC;EACF,CAAC;EACDE,UAAU,EAAE;IACVpB,cAAc;IACdC,UAAU;IACVC,OAAO;IACPC,WAAW;IACXC;EACF;AACF,CAAC","names":["TodayKnowledge","LittleTool","MemoCom","QuestionCom","BottomColumn","data","searchValue","show","column","computed","pet_type","$store","getters","choiceAnimal","user","state","methods","changed","item","pet_typeid","components"],"sourceRoot":"","sources":["C:\\Code\\VScode\\VUE\\项目\\01-xiaoms\\src\\views\\XiaomengShu.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <!-- <div class=\"top-nav\"> -->\r\n    <!-- 头部导航栏 -->\r\n    <nav class=\"top-nav\">\r\n      <div class=\"choice-animal\" @click=\"show = true\">\r\n        <img :src=\"choiceAnimal.url\" alt=\"\" />\r\n        <span>{{ choiceAnimal.name }}站</span>\r\n      </div>\r\n      <van-search\r\n        v-model=\"searchValue\"\r\n        placeholder=\"枫趣\"\r\n        shape=\"round\"\r\n        left-icon=\"\"\r\n        right-icon=\"search\"\r\n        class=\"top-search\"\r\n      >\r\n      </van-search>\r\n      <span><van-icon name=\"chat-o\" size=\"6vw\" /></span>\r\n    </nav>\r\n    <!-- 头像显示 -->\r\n    <div class=\"loginbac\">\r\n      <div class=\"login\">\r\n        <van-image round width=\"5rem\" height=\"5rem\" :src=\"user.headUrl\" />\r\n        <p>我们想要更懂你</p>\r\n        <van-button type=\"warning\" :round=\"true\">立即登录</van-button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n  <!-- 中间小组件 -->\r\n  <div class=\"middle-nav\">\r\n    <today-knowledge></today-knowledge>\r\n    <little-tool></little-tool>\r\n    <memo-com></memo-com>\r\n    <question-com></question-com>\r\n  </div>\r\n\r\n  <!-- 广告推荐  瀑布流 -->\r\n  <bottom-column></bottom-column>\r\n\r\n  <!-- 初始不显示的页面 -->\r\n  <van-popup v-model:show=\"show\" position=\"top\" round>\r\n    <van-list class=\"changeainmal\">\r\n      <van-cell\r\n        v-for=\"item in 2\"\r\n        :key=\"item\"\r\n        :border=\"false\"\r\n        @click=\"changed(item)\"\r\n      >\r\n        <img\r\n          :src=\"$store.state.animalUrl[item - 1]\"\r\n          alt=\"\"\r\n          :class=\"\r\n            $store.state.animalUrl[item - 1] == choiceAnimal.url\r\n              ? 'pitchOn-animal'\r\n              : ''\r\n          \"\r\n        />\r\n        <span>{{ $store.state.animalName[item - 1] }}站</span>\r\n      </van-cell>\r\n    </van-list>\r\n    <van-row>\r\n      <van-col span=\"12\" class=\"btn1\">默认按钮</van-col>\r\n      <van-col span=\"12\" class=\"btn1\">默认按钮</van-col>\r\n    </van-row>\r\n  </van-popup>\r\n</template>\r\n\r\n<script>\r\nimport TodayKnowledge from \"@/views/xiaomengshu/TodayKnowledge.vue\";\r\nimport LittleTool from \"@/views/xiaomengshu/LittleTool.vue\";\r\nimport MemoCom from \"@/views/xiaomengshu/MemoCom.vue\";\r\nimport QuestionCom from \"@/views/xiaomengshu/QuestionCom.vue\";\r\nimport BottomColumn from \"@/views/xiaomengshu/BottomColumn.vue\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      searchValue: \"\",\r\n      show: false,\r\n      column: [],\r\n    };\r\n  },\r\n  computed: {\r\n    pet_type() {\r\n      return this.$store.getters.pet_type;\r\n    },\r\n    choiceAnimal() {\r\n      return this.$store.getters.choiceAnimal;\r\n    },\r\n    user() {\r\n      return this.$store.state.user;\r\n    },\r\n  },\r\n  methods: {\r\n    changed(item) {\r\n      this.show = false;\r\n      // close();\r\n      this.$store.state.pet_typeid = item - 1;\r\n    },\r\n  },\r\n  components: {\r\n    TodayKnowledge,\r\n    LittleTool,\r\n    MemoCom,\r\n    QuestionCom,\r\n    BottomColumn,\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.top-nav {\r\n  display: flex;\r\n  height: 13vw;\r\n  line-height: 13vw;\r\n  text-align: center;\r\n  justify-content: space-around;\r\n  background-color: white;\r\n  padding: 0 3vw;\r\n\r\n  .top-search {\r\n    flex-grow: 1;\r\n  }\r\n  .choice-animal {\r\n    // flex-grow: 1;\r\n    height: 13vw;\r\n    line-height: 13vw;\r\n    text-align: center;\r\n    img {\r\n      border-radius: 50%;\r\n      vertical-align: middle; //解决图片和文字同行居中\r\n      width: 8vw;\r\n      height: 8vw;\r\n      margin: 0 1vw;\r\n    }\r\n  }\r\n}\r\n.changeainmal {\r\n  .van-cell__value {\r\n    height: 20vw;\r\n    line-height: 20vw;\r\n    text-align: center;\r\n    img {\r\n      border-radius: 50%;\r\n      vertical-align: top; //解决图片和文字同行居中\r\n      width: 10vw;\r\n      height: 10vw;\r\n      margin: 0 1vw;\r\n    }\r\n  }\r\n}\r\n.btn1 {\r\n  width: 100%;\r\n  border: 1px solid #e7e7e7;\r\n  height: 13vw;\r\n  line-height: 13vw;\r\n  text-align: center;\r\n  vertical-align: auto;\r\n}\r\n.loginbac {\r\n  width: 100vw;\r\n  height: 70vw;\r\n  background-image: url(\"https://static.epetbar.com/epet_wap_img/opgc/version_v2/default_bg.png\");\r\n  text-align: center;\r\n  vertical-align: middle;\r\n  display: table-cell;\r\n}\r\n.middle-nav {\r\n  margin-top: -10vw;\r\n  div {\r\n    background-color: white;\r\n    border-radius: 20px;\r\n    margin: 3vw auto;\r\n    width: 95vw;\r\n  }\r\n}\r\n.pitchOn-animal {\r\n  border: 3px solid #74be5f;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}
{"ast":null,"code":"export default {\n  props: {\n    cid: {\n      type: Number,\n      default: 0\n    },\n    detailList: {\n      type: Array,\n      default: () => []\n    }\n  },\n  inject: ['ids', 'no_ids'],\n  data() {\n    return {\n      allids: [],\n      showDialog: false,\n      showImg: {}\n    };\n  },\n  methods: {\n    changeCid(cid, index, type) {\n      // console.log(\"type:\", type)\n      if (type === 0) this.allids[index][0] = cid;else if (type === 1) {\n        let ls = this.allids[index].indexOf(cid);\n        if (ls == -1) this.allids[index].push(cid);else {\n          this.allids[index].splice(ls, 1);\n        }\n      } else if (type === 2) {\n        this.allids[index][0] = cid;\n        this.showDialog = false;\n      } else {\n        let ls = this.allids[index].indexOf(cid);\n        // console.log(\"ls:\", ls)\n        if (ls == -1) this.allids[index].push(cid);else {\n          this.allids[index].splice(ls, 1);\n        }\n        this.showDialog = false;\n      }\n    },\n    imgClass(cid, index) {\n      if (this.allids[index].indexOf(cid) != -1) return true;else return false;\n    },\n    changeImgCid(btn, index, type) {\n      if (this.imgClass(btn.cid, index)) {\n        this.changeCid(btn.cid, index, btn.type);\n        return;\n      }\n      this.showDialog = true;\n      this.showImg = {\n        ...btn,\n        index,\n        type\n      };\n    }\n  },\n  watch: {\n    detailList() {\n      console.log(this.detailList);\n      if (!this.detailList) return;\n      for (let i = 0; i < this.allids.length; i++) {\n        this.ids.push(...this.allids[i]);\n      }\n      this.allids = [];\n      for (let i = 0; i < this.detailList.length; i++) {\n        this.allids.push([]);\n        let {\n          list\n        } = this.detailList[i];\n        for (let j = 0; j < list.length; j++) {\n          if (this.ids.indexOf(list[j].cid) == -1) {\n            this.no_ids.push(list[j].cid);\n          }\n        }\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAyDA,eAAe;EACXA,KAAK,EAAE;IACHC,GAAG,EAAE;MACDC,IAAI,EAAEC,MAAM;MACZC,OAAO,EAAE;IACb,CAAC;IACDC,UAAU,EAAE;MACRH,IAAI,EAAEI,KAAK;MACXF,OAAO,EAAE,MAAM;IACnB;EACJ,CAAC;EACDG,MAAM,EAAE,CAAC,KAAK,EAAE,QAAQ,CAAC;EACzBC,IAAI,GAAG;IACH,OAAO;MACHC,MAAM,EAAE,EAAE;MACVC,UAAU,EAAE,KAAK;MACjBC,OAAO,EAAE,CACT;IACJ,CAAC;EACL,CAAC;EACDC,OAAO,EAAE;IACLC,SAAS,CAACZ,GAAG,EAAEa,KAAK,EAAEZ,IAAI,EAAE;MACxB;MACA,IAAIA,IAAG,KAAM,CAAC,EAAE,IAAI,CAACO,MAAM,CAACK,KAAK,CAAC,CAAC,CAAC,IAAIb,GAAG,MACtC,IAAIC,IAAG,KAAM,CAAC,EAAE;QACjB,IAAIa,EAAC,GAAI,IAAI,CAACN,MAAM,CAACK,KAAK,CAAC,CAACE,OAAO,CAACf,GAAG,CAAC;QACxC,IAAIc,EAAC,IAAK,CAAC,CAAC,EAAE,IAAI,CAACN,MAAM,CAACK,KAAK,CAAC,CAACG,IAAI,CAAChB,GAAG,CAAC,MACrC;UACD,IAAI,CAACQ,MAAM,CAACK,KAAK,CAAC,CAACI,MAAM,CAACH,EAAE,EAAE,CAAC,CAAC;QACpC;MACJ,OAEK,IAAIb,IAAG,KAAM,CAAC,EAAE;QACjB,IAAI,CAACO,MAAM,CAACK,KAAK,CAAC,CAAC,CAAC,IAAIb,GAAG;QAC3B,IAAI,CAACS,UAAS,GAAI,KAAI;MAC1B,OACK;QACD,IAAIK,EAAC,GAAI,IAAI,CAACN,MAAM,CAACK,KAAK,CAAC,CAACE,OAAO,CAACf,GAAG,CAAC;QACxC;QACA,IAAIc,EAAC,IAAK,CAAC,CAAC,EAAE,IAAI,CAACN,MAAM,CAACK,KAAK,CAAC,CAACG,IAAI,CAAChB,GAAG,CAAC,MACrC;UACD,IAAI,CAACQ,MAAM,CAACK,KAAK,CAAC,CAACI,MAAM,CAACH,EAAE,EAAE,CAAC,CAAC;QACpC;QACA,IAAI,CAACL,UAAS,GAAI,KAAI;MAC1B;IACJ,CAAC;IACDS,QAAQ,CAAClB,GAAG,EAAEa,KAAK,EAAE;MACjB,IAAI,IAAI,CAACL,MAAM,CAACK,KAAK,CAAC,CAACE,OAAO,CAACf,GAAG,KAAK,CAAC,CAAC,EACrC,OAAO,IAAG,MAEV,OAAO,KAAI;IAEnB,CAAC;IACDmB,YAAY,CAACC,GAAG,EAAEP,KAAK,EAAEZ,IAAI,EAAE;MAC3B,IAAI,IAAI,CAACiB,QAAQ,CAACE,GAAG,CAACpB,GAAG,EAAEa,KAAK,CAAC,EAAE;QAC/B,IAAI,CAACD,SAAS,CAACQ,GAAG,CAACpB,GAAG,EAAEa,KAAK,EAAEO,GAAG,CAACnB,IAAI,CAAC;QACxC;MACJ;MACA,IAAI,CAACQ,UAAS,GAAI,IAAI;MACtB,IAAI,CAACC,OAAM,GAAI;QAAE,GAAGU,GAAG;QAAEP,KAAK;QAAEZ;MAAK;IACzC;EACJ,CAAC;EACDoB,KAAK,EAAE;IACHjB,UAAU,GAAG;MACTkB,OAAO,CAACC,GAAG,CAAC,IAAI,CAACnB,UAAU;MAC3B,IAAI,CAAC,IAAI,CAACA,UAAU,EAChB;MACJ,KAAK,IAAIoB,IAAI,CAAC,EAAEA,IAAI,IAAI,CAAChB,MAAM,CAACiB,MAAM,EAAED,CAAC,EAAE,EAAE;QACzC,IAAI,CAACE,GAAG,CAACV,IAAI,CAAC,GAAG,